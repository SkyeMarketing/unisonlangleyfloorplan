{
  "dependencies": {
    "@netlify/functions": "^1.0.0",
    "@remix-run/netlify": "^1.5.1",
    "@remix-run/node": "^1.5.1",
    "@remix-run/react": "^1.5.1",
    "@sendgrid/mail": "^7.7.0",
    "cross-env": "^7.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.5.1",
    "@remix-run/eslint-config": "^1.5.1",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "autoprefixer": "^10.4.7",
    "concurrently": "^7.2.2",
    "eslint": "^8.15.0",
    "postcss": "^8.4.14",
    "tailwindcss": "^3.1.4",
    "typescript": "^4"
  },
  "engines": {
    "node": ">=14"
  },
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev-css\" \"npm run dev-server\"",
    "dev-css": "npx tailwindcss -w -i styles/app.css -o app/styles/app.css",
    "dev-server": "cross-env NODE_ENV=development netlify dev ",
    "build": "npm run build-css && npm run build-build",
    "build-build": "netlify build",
    "build-css": "tailwindcss -m -i styles/app.css -o app/styles/app.css",
    "build-server": "cross-env NODE_ENV=production netlify dev"
  },
  "sideEffects": false
}